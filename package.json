{
  "name": "fynocrat-landing",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "tw:build": "npx @tailwindcss/cli -i ./app/tailwind.css -o ./app/tailwind-output.css",
    "tw:watch": "npx @tailwindcss/cli -i ./app/tailwind.css -o ./app/tailwind-output.css --watch",
    "dev": "start /B npm run tw:watch & npm run dev:app",
    "dev:app": "react-router dev"
  },
  "dependencies": {
    "@fontsource/poppins": "^5.2.7",
    "@mantine/core": "^8.3.8",
    "@mantine/hooks": "^8.3.8",
    "@mantine/modals": "^8.3.8",
    "@mantine/notifications": "^8.3.8",
    "@react-router/node": "^7.9.2",
    "@react-router/serve": "^7.9.2",
    "@remix-run/node": "^2.17.2",
    "isbot": "^5.1.31",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-google-recaptcha": "^3.1.0",
    "react-router": "^7.9.2"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.2",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-google-recaptcha": "^2.1.9",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
